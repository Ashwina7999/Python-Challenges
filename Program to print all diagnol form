
a=[[1,2,3],
   [4,5,6],
   [7,8,9]]
x=0
list1=[]
for k in range(6):
    list2=[]
    for i in range(len(a)):
        for j in range(len(a[i])):
            if(i+j==x):
                list2.append(a[i][j])
        if(list2 not in list1):        
            list1.append(list2)        
    x+=1   
for i in list1:
    i=i[::-1]
    if(i!=[]):
        print(*i,end=' ')
        
output:
1 4 2 7 5 3 8 6 9
