a=798
while(1):
    temp=str(a)
    s=sorted(temp)
    if(''.join(temp)==''.join(s)):
        print(temp)
        break
    else:
        b=int(a)-1
        a=b





n=4
a=[n]*n
for i in a:
    print(i,end=" ")
print()
list1=[a]*n
x=1
for i in list1:
    i[-2]=x
    for j in i:
        print(j,end=" ")
    print()
    x+=1
